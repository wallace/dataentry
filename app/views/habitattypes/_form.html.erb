Enter new habitat type information:<br />
<%= form_for @habitattype do |f| -%>
  <%= render 'shared/errormessages', :object => f.object %>
    habitat_type_id: <%= f.text_field :habitat_type_id %> <br />
    habitat_name: <%= f.text_field :habitat_name %> <br />
    habitat_descr: <%= f.text_field :habitat_descr %> <br />
    <%= f.submit %>
  <% end -%>
  <hr />
List of all habitat types: (click on name to edit) <br />
<% if !@habitattypes.blank? %>
  <% for item in @habitattypes %> 
    <%= item.habitat_type_id %>, <%= link_to item.habitat_name, edit_habitattype_path(item)%>, <%= item.habitat_descr %> <br />
  <% end %>
<% else %>

<% end %>

 <%= render 'shared/quick_links' %>
